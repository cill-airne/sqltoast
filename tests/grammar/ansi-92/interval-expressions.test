# Simple interval value expression with non-second interval qualifier
>SELECT a YEAR FROM t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - type: INTERVAL_EXPRESSION
            interval_expression:
              left:
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: YEAR
        referenced_tables:
          - type: TABLE
            table:
              name: t1
