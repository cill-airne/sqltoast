# Simple interval value expression with non-second interval qualifier
>SELECT a YEAR FROM t1
statements:
  - type: SELECT
    query:
      selected_columns:
        - derived_column:
            value_expression:
              type: INTERVAL_EXPRESSION
              interval_expression:
                left:
                  term:
                    left:
                      factor:
                        primary: column-reference[a] YEAR
      referenced_tables:
        - t1
