# interval value expression that operates on an interval term with a numeric
# factor
>SELECT a YEAR * 12, a YEAR / 2, a MONTH / (12 / b) FROM t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - derived_column:
              value_expression:
                type: INTERVAL_EXPRESSION
                interval_expression:
                  left:
                    term:
                      left:
                        factor:
                          primary:
                            value: column-reference[a]
                            qualifier:
                              start:
                                interval: YEAR
                      op: MULTIPLY
                      right:
                        factor:
                          primary:
                            type: VALUE
                            value: literal[12]
          - derived_column:
              value_expression:
                type: INTERVAL_EXPRESSION
                interval_expression:
                  left:
                    term:
                      left:
                        factor:
                          primary:
                            value: column-reference[a]
                            qualifier:
                              start:
                                interval: YEAR
                      op: DIVIDE
                      right:
                        factor:
                          primary:
                            type: VALUE
                            value: literal[2]
          - derived_column:
              value_expression:
                type: INTERVAL_EXPRESSION
                interval_expression:
                  left:
                    term:
                      left:
                        factor:
                          primary:
                            value: column-reference[a]
                            qualifier:
                              start:
                                interval: MONTH
                      op: DIVIDE
                      right:
                        factor:
                          primary:
                            type: VALUE
                            value: (numeric-expression[literal[12] / column-reference[b]])
        referenced_tables:
          - t1
# interval value expression that uses addition and subtraction operators on
# interval terms
>SELECT a YEAR + 12 YEAR, created_on DAY TO SECOND - updated_on DAY TO SECOND from t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - derived_column:
              value_expression:
                type: INTERVAL_EXPRESSION
                interval_expression:
                  left:
                    term:
                      left:
                        factor:
                          primary:
                            value: column-reference[a]
                            qualifier:
                              start:
                                interval: YEAR
                  op: ADD
                  right:
                    term:
                      left:
                        factor:
                          primary:
                            value: literal[12]
                            qualifier:
                              start:
                                interval: YEAR
          - derived_column:
              value_expression:
                type: INTERVAL_EXPRESSION
                interval_expression:
                  left:
                    term:
                      left:
                        factor:
                          primary:
                            value: column-reference[created_on]
                            qualifier:
                              start:
                                interval: DAY
                              end:
                                interval: SECOND
                  op: SUBTRACT
                  right:
                    term:
                      left:
                        factor:
                          primary:
                            value: column-reference[updated_on]
                            qualifier:
                              start:
                                interval: DAY
                              end:
                                interval: SECOND
        referenced_tables:
          - t1
