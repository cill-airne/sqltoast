# interval value expression that operates on an interval term with a numeric
# factor
>SELECT a YEAR * 12, a YEAR / 2, a MONTH / (12 / b) FROM t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - type: INTERVAL_EXPRESSION
            interval_expression:
              left:
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: YEAR
                op: MULTIPLY
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: YEAR
          - type: INTERVAL_EXPRESSION
            interval_expression:
              left:
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: YEAR
                op: DIVIDE
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: YEAR
          - type: INTERVAL_EXPRESSION
            interval_expression:
              left:
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: MONTH
                op: DIVIDE
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: MONTH
        referenced_tables:
          - type: TABLE
            table:
              name: t1
# interval value expression that uses addition and subtraction operators on
# interval terms
>SELECT a YEAR + 12 YEAR, created_on DAY TO SECOND - updated_on DAY TO SECOND from t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - type: INTERVAL_EXPRESSION
            interval_expression:
              left:
                left:
                  primary:
                    value: column-reference[a]
                    qualifier:
                      start:
                        interval: YEAR
              op: ADD
              right:
                left:
                  primary:
                    value: literal[12]
                    qualifier:
                      start:
                        interval: YEAR
          - type: INTERVAL_EXPRESSION
            interval_expression:
              left:
                left:
                  primary:
                    value: column-reference[created_on]
                    qualifier:
                      start:
                        interval: DAY
                      end:
                        interval: SECOND
              op: SUBTRACT
              right:
                left:
                  primary:
                    value: column-reference[updated_on]
                    qualifier:
                      start:
                        interval: DAY
                      end:
                        interval: SECOND
        referenced_tables:
          - type: TABLE
            table:
              name: t1
