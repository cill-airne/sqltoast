>INSERT INTO t1 (a, b) VALUES (1, 2)
statements:
  - type: INSERT
    table_name: t1
    columns:
      - a
      - b
    values:
      - row_value_constructor:
          type: ELEMENT
          element:
            type: VALUE_EXPRESSION
            value_expression:
              type: NUMERIC_EXPRESSION
              numeric_expression:
                term:
                  left:
                    factor:
                      primary:
                        type: VALUE
                        value: literal[1]
      - row_value_constructor:
          type: ELEMENT
          element:
            type: VALUE_EXPRESSION
            value_expression:
              type: NUMERIC_EXPRESSION
              numeric_expression:
                term:
                  left:
                    factor:
                      primary:
                        type: VALUE
                        value: literal[2]
