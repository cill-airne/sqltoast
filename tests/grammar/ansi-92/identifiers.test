# Simple identifier all alphanumeric
>SELECT * FROM t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - derived_column:
              asterisk: true
        referenced_tables:
          - type: TABLE
            table:
              name: t1
# Identifier includes an underscore
>SELECT * FROM t1_backup
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
