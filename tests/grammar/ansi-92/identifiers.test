# Identifier includes a prefix and a star projection
>SELECT t1.* FROM t1
statements:
  - type: SELECT
    select_statement:
      query:
        selected_columns:
          - derived_column:
              value_expression:
                type: NUMERIC_EXPRESSION
                numeric_expression:
                  term:
                    left:
                      factor:
                        primary:
                          type: VALUE
                          value: column-reference[t1.*]
        referenced_tables:
          - type: TABLE
            table:
              name: t1
