                                                                      numeric_expression:
                                                                        left:
                                                                          left:
                                                                            primary:
                                                                              type: VALUE
                                                                              value:
                                                                                primary:
                                                                                  type: COLUMN_REFERENCE
                                                                                  column_reference: t2.t1_id
                                                                right:
                                                                  type: ELEMENT
                                                                  element:
                                                                    type: VALUE_EXPRESSION
                                                                    value_expression:
                                                                      type: NUMERIC_EXPRESSION
                                                                      numeric_expression:
                                                                        left:
                                                                          left:
                                                                            primary:
                                                                              type: VALUE
                                                                              value:
                                                                                primary:
                                                                                  type: COLUMN_REFERENCE
                                                                                  column_reference: t1.id
