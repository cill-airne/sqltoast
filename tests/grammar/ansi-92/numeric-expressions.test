# Use of CHAR_LENGTH and CHARACTER_LENGTH numeric functions
>SELECT CHAR_LENGTH(a), CHARACTER_LENGTH(a) + 1 FROM t1
statements:
  - type: SELECT
    query:
      selected_columns:
        - derived_column:
            value_expression:
              type: NUMERIC_EXPRESSION
              numeric_expression:
                term:
                  left:
                    factor:
                      primary:
                        type: FUNCTION
                        function: char-length[column-reference[a]]
        - derived_column:
            value_expression:
              type: NUMERIC_EXPRESSION
              numeric_expression:
                left:
                  term:
                    left:
                      factor:
                        primary:
                          type: FUNCTION
                          function: char-length[column-reference[a]]
                op: ADD
                right:
                  term:
                    left:
                      factor:
                        primary:
                          type: VALUE
                          value: literal[1]
      referenced_tables:
        - t1
